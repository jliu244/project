<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.courseImportRubricController">
   <children>
      <AnchorPane prefHeight="247.0" prefWidth="401.0">
         <children>
            <Label layoutX="60.0" layoutY="51.0" prefHeight="36.0" prefWidth="165.0" text="Import Rubric">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button layoutX="253.0" layoutY="51.0" mnemonicParsing="false" onAction="#chooseFile" onMouseClicked="#chooseFile" text="Choose">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button layoutX="81.0" layoutY="180.0" mnemonicParsing="false" onAction="#Import" text="Import">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button layoutX="231.0" layoutY="180.0" mnemonicParsing="false" onAction="#cancel" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <TextField fx:id="inputFilePath" layoutX="59.0" layoutY="112.0" prefHeight="23.0" prefWidth="282.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
